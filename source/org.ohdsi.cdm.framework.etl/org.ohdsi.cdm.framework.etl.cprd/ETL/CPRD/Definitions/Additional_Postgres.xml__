			where t2._enttype is null
			and t1.data_fields > 3 
		
		), w_entity_m AS (
			select *
			from w_entity as t
			where t.data4_lkup = 'Medical Dictionary'
		
		), w_entity_p AS (
			select *
			from w_entity as t
			where t.data4_lkup = 'Product Dictionary'
			
		), w_entity_oth AS (
