			left join (SELECT * FROM (VALUES ('Medical Dictionary'), ('Product Dictionary')) AS t (_data3_lkup)) AS t2 on t1.data3_lkup = t2._data3_lkup
			where t2._data3_lkup is null
			
		), w_lookup AS(
			select lu.code, lu.text, lt.name  
			from {sc}.lookup lu
			join {sc}.lookuptype lt on lt.lookup_type_id = lu.lookup_type_id
		)
		select distinct 
