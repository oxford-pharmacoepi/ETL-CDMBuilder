			ac.eventdate,
			ac.staffid,
			m.readcode as value_as_string,
			null as value_as_number,
			case 
				when e.data3_lkup = 'dd/mm/yyyy' then ac.data3
			end as value_as_date,
			'' as unit_source_value,
			MOD(ac.patid, 100000) as care_site_id,
			con.constype,
			concat(ac.temp_person_id,to_char(ac.eventdate,'YYYYMMDD'), coalesce(con.constype, 99))::numeric as visit_occurrence_id,
			'' as qualifier_source_value,
			null as qualifier_concept_id,
			e.data3 as data,
			concat(ac.enttype::varchar, '-', e.category, '-', e.description, '-', e.data3)  as source_value
		from ac 
		join w_entity_m e on e.enttype = ac.enttype 	
		join {sc}.consultation con on ac.patid = con.patid and ac.consid = con.consid 
								
		left join {sc}.medical m on m.medcode::text = ac.data3 
		where con.eventdate is not null
		
		UNION
		
		select distinct 
			ac.patid,
			ac.eventdate,
			ac.staffid,
			p.gemscriptcode as value_as_string,
			null as value_as_number,
			case 
				when e.data3_lkup = 'dd/mm/yyyy' then ac.data3
			end as value_as_date,
			'' as unit_source_value,
			MOD(ac.patid, 100000) as care_site_id,
			con.constype,
			concat(ac.temp_person_id,to_char(ac.eventdate,'YYYYMMDD'), coalesce(con.constype, 99))::numeric as visit_occurrence_id,
			'' as qualifier_source_value,
			null as qualifier_concept_id,
			e.data3 as data,
			concat(ac.enttype::varchar, '-', e.category, '-', e.description, '-', e.data3)  as source_value
		from ac 
		join w_entity_p e on e.enttype = ac.enttype 	
		join {sc}.consultation con on ac.patid = con.patid and ac.consid = con.consid 

		left join {sc}.product p on p.prodcode::text = ac.data3 
		where con.eventdate is not null
		
		UNION
		
		select distinct 
			ac.patid,
			ac.eventdate,
			ac.staffid,
			ll.text as value_as_string,
			ac.data3 as value_as_number,
			case 
				when e.data3_lkup = 'dd/mm/yyyy' then ac.data3
			end as value_as_date,
			'' as unit_source_value,
			MOD(ac.patid, 100000) as care_site_id,
			con.constype,
			concat(ac.temp_person_id,to_char(ac.eventdate,'YYYYMMDD'), coalesce(con.constype, 99))::numeric as visit_occurrence_id,
			'' as qualifier_source_value,
			null as qualifier_concept_id,
			e.data3 as data,
			concat(ac.enttype::varchar, '-', e.category, '-', e.description, '-', e.data3)  as source_value
		from ac 
		join w_entity_oth e on e.enttype = ac.enttype 	
		join {sc}.consultation con on ac.patid = con.patid and ac.consid = con.consid 
		
		left join w_lookup ll on ll.code::text = ac.data3 and ll.name = e.data3_lkup		
		where con.eventdate is not null		

		order by patid
		GO
