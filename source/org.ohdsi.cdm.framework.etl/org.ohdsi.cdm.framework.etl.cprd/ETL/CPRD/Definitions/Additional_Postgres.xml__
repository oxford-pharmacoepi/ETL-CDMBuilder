<QueryDefinition>
	<Query>
		With ch AS(
		select person_id, temp_person_id
		from {sc}.chunk_person
		where chunk_id = {0}

		), w_additional AS(
		select a.patid, a.adid, a.enttype, a.data1, ch.temp_person_id
		from {sc}.additional a
		join ch on a.patid=ch.person_id

		), ac AS(
		select DISTINCT
		a.temp_person_id, a.patid, a.enttype, a.data1,
		c.staffid, c.consid, c.eventdate
		from {sc}.clinical c
		join w_additional a on c.patid = a.patid and c.adid = a.adid
		where c.eventdate is not null

		), w_entity AS (
		select t1.enttype, t1.category, t1.description, t1.data1, t1.data1_lkup, t1.data_fields
		from {sc}.entity as t1
		left join (SELECT * FROM (VALUES (72), (116), (372), (78)) AS t (_enttype)) AS t2 on t1.enttype = t2._enttype
		where t2._enttype is null
		and t1.data_fields > 0
			
		), w_entity_m AS (
		select *
		from w_entity as t
		where t.data1_lkup = 'Medical Dictionary'
		
		), w_entity_p AS (
		select *
		from w_entity as t
		where t.data1_lkup = 'Product Dictionary'
			
		), w_entity_oth AS (
		select *
		from w_entity as t1
		left join (SELECT * FROM (VALUES ('Medical Dictionary'), ('Product Dictionary')) AS t (_data1_lkup)) AS t2 on t1.data1_lkup = t2._data1_lkup
		where t2._data1_lkup is null
			
		), w_lookup AS(
		select lu.code, lu.text, lt.name
		from {sc}.lookup lu
		join {sc}.lookuptype lt on lt.lookup_type_id = lu.lookup_type_id

		)
		select distinct
		ac.patid,
		ac.eventdate, 
		ac.staffid,
		m.readcode as value_as_string,
		null as value_as_number,
		case
		when e.data1_lkup = 'dd/mm/yyyy' then ac.data1
		end as value_as_date,
		case
		when ac.enttype in (13,488) then 'kg' --enttype 13 is weight, enttype 488 is weight loss
		when ac.enttype = 476 then 'cm' --enttype 476 is waist circumference
		when ac.enttype in (119,61,60,120) then 'week' --These enttypes relate to weeks gestation of fetus
		when ac.enttype = 14 then 'm' --enttype 14 is height
		else '' --These units have to be hard coded as they do not have a unit lookup
		end as unit_source_value,
		MOD(ac.patid, 100000) as care_site_id,
		con.constype,
		concat(ac.temp_person_id, to_char(ac.eventdate,'YYYYMMDD'), coalesce(con.constype, 99))::numeric as visit_occurrence_id,
		'' as qualifier_source_value,
		null as qualifier_concept_id,
		e.data1 as data,
		concat(ac.enttype::varchar, '-', e.category, '-', e.description, '-', e.data1)  as source_value
		from ac
		join w_entity_m e on e.enttype = ac.enttype
		join {sc}.consultation con on ac.patid = con.patid and ac.consid = con.consid

		left join {sc}.medical m on m.medcode::text = ac.data1 
		where con.eventdate is not null

		UNION
		
		select distinct
		ac.patid,
		ac.eventdate, 
		ac.staffid,
		p.gemscriptcode as value_as_string,
		null as value_as_number,
		case
		when e.data1_lkup = 'dd/mm/yyyy' then ac.data1
		end as value_as_date,
		case
		when ac.enttype in (13,488) then 'kg' --enttype 13 is weight, enttype 488 is weight loss
		when ac.enttype = 476 then 'cm' --enttype 476 is waist circumference
		when ac.enttype in (119,61,60,120) then 'week' --These enttypes relate to weeks gestation of fetus
		when ac.enttype = 14 then 'm' --enttype 14 is height
		else '' --These units have to be hard coded as they do not have a unit lookup
		end as unit_source_value,
		MOD(ac.patid, 100000) as care_site_id,
		con.constype,
		concat(ac.temp_person_id,to_char(ac.eventdate,'YYYYMMDD'), coalesce(con.constype, 99))::numeric as visit_occurrence_id,
		'' as qualifier_source_value,
		null as qualifier_concept_id,
		e.data1 as data,
		concat(ac.enttype::varchar, '-', e.category, '-', e.description, '-', e.data1)  as source_value
		from ac
		join w_entity_p e on e.enttype = ac.enttype 
		join {sc}.consultation con on ac.patid = con.patid and ac.consid = con.consid
		left join {sc}.product p on p.prodcode::text = ac.data1
		where con.eventdate is not null
	
		UNION
		
		select distinct
		ac.patid,
		ac.eventdate, 
		ac.staffid,
		ll.text as value_as_string,
		ac.data1 as value_as_number,
		case
		when e.data1_lkup = 'dd/mm/yyyy' then ac.data1
		end as value_as_date,
		case
		when ac.enttype in (13,488) then 'kg' --enttype 13 is weight, enttype 488 is weight loss
		when ac.enttype = 476 then 'cm' --enttype 476 is waist circumference
		when ac.enttype in (119,61,60,120) then 'week' --These enttypes relate to weeks gestation of fetus
		when ac.enttype = 14 then 'm' --enttype 14 is height
		else '' --These units have to be hard coded as they do not have a unit lookup
		end as unit_source_value,
		MOD(ac.patid, 100000) as care_site_id,
		con.constype,
		concat(ac.temp_person_id,to_char(ac.eventdate,'YYYYMMDD'), coalesce(con.constype, 99))::numeric as visit_occurrence_id,
		'' as qualifier_source_value,
		null as qualifier_concept_id,
		e.data1 as data,
		concat(ac.enttype::varchar, '-', e.category, '-', e.description, '-', e.data1)  as source_value
		from ac
		join w_entity_oth e on e.enttype = ac.enttype 
		join {sc}.consultation con on ac.patid = con.patid and ac.consid = con.consid

		left join w_lookup ll on ll.code::text = ac.data1 and ll.name = e.data1_lkup
		where con.eventdate is not null

		order by patid
		GO
		With ch AS(
			select person_id, temp_person_id
			from {sc}.chunk_person
			where chunk_id = {0}

		), w_additional AS(
			select a.patid, a.adid, a.enttype, a.data2, ch.temp_person_id 
			from {sc}.additional a
			join ch on a.patid=ch.person_id
			
		), ac AS(
			select DISTINCT
				a.temp_person_id, a.patid, a.enttype, a.data2,
				c.staffid, c.consid, c.eventdate 
			from {sc}.clinical c
			join w_additional a on c.patid = a.patid and c.adid = a.adid
			where c.eventdate is not null
				
		), w_entity AS (	
			select t1.enttype, t1.category, t1.description, t1.data2, t1.data2_lkup, t1.data_fields
			from {sc}.entity as t1
			left join (SELECT * FROM (VALUES (72), (116), (78), (60), (119), (120)) AS t (_enttype)) AS t2 on t1.enttype = t2._enttype
			where t2._enttype is null
			and t1.data_fields > 1 
			
		), w_entity_m AS (
			select *
			from w_entity as t
			where t.data2_lkup = 'Medical Dictionary'
		
		), w_entity_p AS (
			select *
			from w_entity as t
			where t.data2_lkup = 'Product Dictionary'
			
		), w_entity_oth AS (
			select *
			from w_entity as t1
			left join (SELECT * FROM (VALUES ('Medical Dictionary'), ('Product Dictionary')) AS t (_data2_lkup)) AS t2 on t1.data2_lkup = t2._data2_lkup
			where t2._data2_lkup is null
		
		), w_lookup AS(
			select lu.code, lu.text, lt.name  
			from {sc}.lookup lu
			join {sc}.lookuptype lt on lt.lookup_type_id = lu.lookup_type_id
		)
		select distinct 
			ac.patid,
			ac.eventdate, 
			ac.staffid,
			m.readcode as value_as_string,
			null as value_as_number,
			case 
				when e.data2_lkup = 'dd/mm/yyyy' then ac.data2
			end as value_as_date,
			case 
				when ac.enttype = 52 then 'hour' --enttype 52 is sleep pattern and this is asking average hours of sleep
				when ac.enttype = 69 then 'week' --enttype 69 is weeks post-natal
				when ac.enttype = 150 then 'day' --enttype 150 is days post-natal
				else ''
			end as unit_source_value,
			MOD(ac.patid, 100000) as care_site_id,
			con.constype,
			concat(ac.temp_person_id,to_char(ac.eventdate,'YYYYMMDD'), coalesce(con.constype, 99))::numeric as visit_occurrence_id,
			'' as qualifier_source_value,
			null as qualifier_concept_id,
			e.data2 as data,
			concat(ac.enttype::varchar, '-', e.category, '-', e.description, '-', e.data2)  as source_value
		from ac 
		join w_entity_m e on e.enttype = ac.enttype 	
		join {sc}.consultation con on ac.patid = con.patid and ac.consid = con.consid 
												
		left join {sc}.medical m on m.medcode::text = ac.data2
		where con.eventdate is not null

		
		UNION
		
		select distinct 
			ac.patid,
			ac.eventdate, 
			ac.staffid,
			p.gemscriptcode as value_as_string,
			null as value_as_number,
			case 
				when e.data2_lkup = 'dd/mm/yyyy' then ac.data2
			end as value_as_date,
			case 
				when ac.enttype = 52 then 'hour' --enttype 52 is sleep pattern and this is asking average hours of sleep
				when ac.enttype = 69 then 'week' --enttype 69 is weeks post-natal
				when ac.enttype = 150 then 'day' --enttype 150 is days post-natal
				else ''
			end as unit_source_value,
			MOD(ac.patid, 100000) as care_site_id,
			con.constype,
			concat(ac.temp_person_id,to_char(ac.eventdate,'YYYYMMDD'), coalesce(con.constype, 99))::numeric as visit_occurrence_id,
			'' as qualifier_source_value,
			null as qualifier_concept_id,
			e.data2 as data,
			concat(ac.enttype::varchar, '-', e.category, '-', e.description, '-', e.data2)  as source_value
		from ac 
		join w_entity_p e on e.enttype = ac.enttype 	
		join {sc}.consultation con on ac.patid = con.patid and ac.consid = con.consid 
					
		left join {sc}.product p on p.prodcode::text = ac.data2
		where con.eventdate is not null

		UNION
		
		select distinct 
			ac.patid,
			ac.eventdate, 
			ac.staffid,
			ll.text as value_as_string,
			ac.data2 as value_as_number,
			case 
				when e.data2_lkup = 'dd/mm/yyyy' then ac.data2
			end as value_as_date,
			case 
				when ac.enttype = 52 then 'hour' --enttype 52 is sleep pattern and this is asking average hours of sleep
				when ac.enttype = 69 then 'week' --enttype 69 is weeks post-natal
				when ac.enttype = 150 then 'day' --enttype 150 is days post-natal
				else ''
			end as unit_source_value,
			MOD(ac.patid, 100000) as care_site_id,
			con.constype,
			concat(ac.temp_person_id,to_char(ac.eventdate,'YYYYMMDD'), coalesce(con.constype, 99))::numeric as visit_occurrence_id,
			'' as qualifier_source_value,
			null as qualifier_concept_id,
			e.data2 as data,
			concat(ac.enttype::varchar, '-', e.category, '-', e.description, '-', e.data2)  as source_value
		from ac 
		join w_entity_oth e on e.enttype = ac.enttype 	
		join {sc}.consultation con on ac.patid = con.patid and ac.consid = con.consid 
					
		left join w_lookup ll on ll.code::text = ac.data2 and ll.name = e.data2_lkup
		where con.eventdate is not null		

		order by patid
		GO
		With ch AS(
			select person_id, temp_person_id
			from {sc}.chunk_person
			where chunk_id = {0}

		), w_additional AS(
